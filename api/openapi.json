    "/login": {
      "post": {
        "description": "Logs in user into application",
        "requestBody": {
          "description": "Sending user email and password to server to authenticate user",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful authentication and login of user into application",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "relation_id": {
                      "type": "string",
                      "format": "uuid"
                    },
                    "forename": {
                      "type": "string"
                    },
                    "surname": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string",
                      "format": "email"
                    },
                    "token": {
                      "type": "string",
                      "format": "jwt"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "The request cannot be computed - client sent invalid data to server endpoint",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "User authentication failed. Invalid authorization credentials",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Something went wrong on the server side - Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
